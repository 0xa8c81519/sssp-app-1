                            "projects/ssp-app/src/assets",
                            {
                                "glob": "**/*",
                                "input": "libs/shared-assets/",
                                "output": "./assets"
                            },
                            {
                                "glob": "favicon.ico",
                                "input": "libs/",
                                "output": "./"
                            }
                        ],
                        "styles": [
